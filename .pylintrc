[MASTER]
extension-pkg-whitelist=ujson
jobs=0
persistent=no


[MESSAGES CONTROL]
disable=fixme,
        cyclic-import,
        duplicate-code,
        missing-docstring,
        raise-missing-from,
        redefined-builtin,
        ungrouped-imports


[BASIC]
include-naming-hint=yes
good-names=_,
           e,
           ep,
           f,
           fd,
           id,
           kw,
           log,
           rv


[DESIGN]
max-args=6


[FORMAT]
expected-line-ending-format=LF
max-line-length=100


[STRING]
check-str-concat-over-line-jumps=yes


[TYPECHECK]
ignored-classes=sqlalchemy.orm.scoping.scoped_session

ignored-modules=capnp.lib.capnp,
                cv2,
                marv_detail.types_capnp,
                marv_nodes.types_capnp,
                marv_pycapnp.types_capnp,
                pydantic


[CLASSES]
exclude-protected=_asdict,
                  _meta,
                  _table_name


[IMPORTS]
known-third-party=build,
                  cv2,
                  enchant
